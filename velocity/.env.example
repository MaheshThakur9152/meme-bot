# Example env vars
PORT=8080
RPC_URL=https://rpc.helius.xyz/?api-key=YOUR_HELIUS_API_KEY
GROQ_API_KEY=YOUR_GROQ_API_KEY
SEND_ENABLED=false
# Keep this empty while in paper trading mode. Fill with base64 secret when enabling live sends.
PAYER_PRIVATE_KEY_BASE58=
# Auto-switch to live when thresholds are met (set to 'true' to allow)
AUTO_SWITCH=false
# Winrate threshold (fraction) to consider switching out of paper mode
PAPER_TO_LIVE_WINRATE=0.8
# Minimum realized trades before considering switch
PAPER_TO_LIVE_MIN_TRADES=50
JUPITER_API_URL=https://quote-api.jup.ag
# DexScreener adapter
USE_DEXSCREENER=true
DEXSCREENER_POLL_MS=2000

# Volume Velocity Manager thresholds (window ms, USD volume, unique buyers)
VVM_WINDOW_MS=300000
VVM_THRESHOLD_VOLUME=5000
VVM_THRESHOLD_BUYERS=20

# On-chain adapter (Helius / QuickNode RPC)
USE_ONCHAIN=true
HELIUS_POLL_MS=2000

# Telegram and Twitter removed (volume-velocity approach recommended)
REGION=us-east-1
LOG_LEVEL=info